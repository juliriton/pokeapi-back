generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id         Int    @id @default(autoincrement())
  name       String @unique
  imageUrl   String @unique
  generation Int
}

model Trainer {
  id        Int    @id @default(autoincrement())
  name      String
  imageUrl   String @unique
  specialty String
}

model Ability {
  id         Int    @id @default(autoincrement())
  name       String
  generation Int
}
